{
	"name": "schmell-view",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "prisma generate --no-engine  && vite build",
		"preview": "vite preview",
		"start": "node build",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"gen": "npx prisma generate --accelerate && npx prisma generate --no-engine && npx prisma migrate dev --name again"
	},
	"devDependencies": {
		"@iconify/svelte": "^3.1.4",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-node": "^1.2.4",
		"@sveltejs/kit": "^1.20.4",
		"@tailwindcss/typography": "^0.5.9",
		"@types/nodemailer": "^6.4.9",
		"@types/papaparse": "^5.3.7",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"daisyui": "^3.5.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^5.2.0",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"sveltekit-superforms": "^1.5.0",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.6",
		"vitest": "^0.25.3",
		"zod": "^3.21.4",
		"zod-prisma-types": "^2.7.9"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^3.0.0",
		"@lucia-auth/oauth": "^2.0.1",
		"@paralleldrive/cuid2": "^2.2.2",
		"@prisma/client": "^5.2.0",
		"@prisma/extension-accelerate": "^0.6.2",
		"@tanstack/svelte-table": "^8.9.5",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"convert-csv-to-json": "^2.0.0",
		"csvtojson": "^2.0.10",
		"googleapis": "^123.0.0",
		"lucia": "^2.0.0",
		"lucide-svelte": "^0.263.0",
		"nodemailer": "^6.9.4",
		"papaparse": "^5.4.1",
		"sveltekit-flash-message": "^2.1.0",
		"tailwind-merge": "^1.14.0",
		"tailwindcss-animate": "^1.0.6"
	}
}
